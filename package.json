{
  "name": "work-in-norway-legacy-proxy",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "module": "es",
  "scripts": {
    "build": "rm -fr distSrc && tsc",
    "start": "node distSrc/server.js",
    "dev": "npm run build && concurrently \"tsc -w\" \"nodemon -w distSrc -r dotenv/config distSrc/server.js\""
  },
  "dependencies": {
    "express": "4.18.2",
    "node-cache": "5.1.2"
  },
  "devDependencies": {
    "@types/express": "4.17.14",
    "@types/node": "18.8.5",
    "concurrently": "7.4.0",
    "dotenv": "16.0.3",
    "eslint": "8.25.0",
    "nodemon": "2.0.20",
    "prettier": "2.7.1",
    "typescript": "4.8.4"
  }
}
