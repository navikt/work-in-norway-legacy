{
  "name": "work-in-norway-legacy",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rm -fr dist && tsc",
    "start": "node dist/server.js",
    "dev": "npm run build && concurrently \"tsc -w\" \"nodemon -w dist dist/server.js\"",
    "scan": "npx linkinator ./static --recurse"
  },
  "dependencies": {
    "express": "4.18.2"
  },
  "devDependencies": {
    "@types/express": "4.17.14",
    "@types/node": "18.11.3",
    "concurrently": "7.5.0",
    "eslint": "8.26.0",
    "glob": "8.0.3",
    "linkinator": "4.1.0",
    "nodemon": "2.0.20",
    "prettier": "2.7.1",
    "typescript": "4.8.4"
  }
}
